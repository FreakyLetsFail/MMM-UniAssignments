# Todoist API Token
# Get your token from: https://todoist.com/prefs/integrations
TODOIST_API_TOKEN=your_api_token_here

# Todoist Project Name (must exist in your Todoist)
TODOIST_PROJECT=UNI

# Label for assignments (optional - leave empty to sync ALL tasks in project)
# If set: only tasks with this label will be synced
# If empty: ALL tasks from sections will be synced
ASSIGNMENT_LABEL=

# Server Configuration
PORT=5000
DEBUG=False
